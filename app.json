{
	"name": "xapp",
	"description": "A simple application demonstrating the basic usage of permissions with NestJS (JWT, Passport, Facebook, Google+, User, Group, Permission)",
	"repository": "https://github.com/rucken/core-nestjs",
	"logo": "https://avatars0.githubusercontent.com/u/32410498?s=200&v=4",
	"keywords": [
		"jwt",
		"oauth",
		"passport",
		"facebook",
		"google+",
		"user",
		"group",
		"permission",
		"contenttype",
		"bootstrap",
		"nestjs",
		"heroku",
		"typedoc"
	],
	"env": {
		"DEBUG": "false",
		"DEMO": "false",
		"DOMAIN": "localhost",
		"PROTOCOL": "https",
		"NODE_MODULES_CACHE": "false",
		"NPM_CONFIG_PRODUCTION": "false",
		"JWT_SECRET_KEY": "secret_key",
		"JWT_EXPIRATION_DELTA": "7 days",
		"JWT_AUTH_HEADER_PREFIX": "JWT",
		"FACEBOOK_CLIENT_ID": "none",
		"FACEBOOK_CLIENT_SECRET": "none",
		"FACEBOOK_OAUTH_REDIRECT_URI": "http://localhost:5000/",
		"GOOGLE_CLIENT_ID": "none",
		"GOOGLE_CLIENT_SECRET": "none",
		"GOOGLE_OAUTH_REDIRECT_URI": "http://localhost:5000/",
		"DATABASE_URL": "sqlite://database/sqlitedb.db"
	}
}
