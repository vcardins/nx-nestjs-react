{
	"npmScope": "xapp",
	"affected": {
		"defaultBase": "master"
	},
	"implicitDependencies": {
		"workspace.json": "*",
		"package.json": {
			"dependencies": "*",
			"devDependencies": "*"
		},
		"tsconfig.base.json": "*",
		"tslint.json": "*",
		"nx.json": "*"
	},
	"tasksRunnerOptions": {
		"default": {
			"runner": "@nrwl/nx-cloud",
			"options": {
				"accessToken": "MTA1OTE5OTYtNmE4Ny00YTRmLTljNDQtMTUyMzMxMjc4MmM5fHJlYWQtd3JpdGU=",
				"cacheableOperations": ["build", "test", "lint", "e2e"],
				"canTrackAnalytics": false,
				"showUsageWarnings": true
			}
		}
	},
	"cli": {
		"defaultCollection": "@nrwl/react"
	},
	"generators": {
		"@nrwl/react": {
			"application": {
				"style": "styled-components",
				"linter": "eslint",
				"babel": true
			},
			"component": {
				"style": "styled-components"
			},
			"library": {
				"style": "styled-components",
				"linter": "eslint"
		},
		"mobile": {
			"tags": []
			}
		}
	},
	"defaultProject": "api"
}
