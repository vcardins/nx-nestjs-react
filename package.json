{
  "name": "xapp",
  "version": "0.0.0",
  "title": "NX NestJs React",
  "description": "Typescript driven monorepo boilerplate",
  "license": "MIT",
  "scripts": {
    "analyze:web:prod": "nx run web:build --prod --stats-json",
    "webpack-analyzer:web": "webpack-bundle-analyzer dist/apps/web/stats.json",
    "build": "nx build",
    "build:all": "run-s affected:prod build:libs build:apps",
    "build:apps": "yarn ng build web --prod",
    "build:libs": "yarn ng build core && yarn ng build auth",
    "lint": "nx workspace-lint && nx run-many --all --target=lint",
    "nx": "nx",
    "update": "nx migrate latest",
    "clean": "./node_modules/.bin/del-cli dist client/docs database/*.db",
    "docs": "./node_modules/.bin/typedoc --jsx --excludeExternals --out client/docs libs",
    "help": "nx help",
    "start": "nx serve",
    "start:dev": "ng run web:serve",
    "start:prod": "ng run web:serve-prod",
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "dep-graph": "nx dep-graph",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "heroku-postbuild": "yarn build",
    "ng": "ng",
    "workspace-schematic": "nx workspace-schematic",
    "pm2:logs:flush": "./node_modules/.bin/pm2 flush --name web",
    "pm2:logs": "./node_modules/.bin/pm2 logs --name web",
    "pm2:start": "cross-env NODE_ENV=production ./node_modules/.bin/pm2 start dist/web/main.js --name web",
    "pm2:stop": "./node_modules/.bin/pm2 kill --name web",
    "postinstall": "sh ./scripts/postinstall.sh",
    "test": "nx test",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "e2e": "nx e2e",
    "typeorm": "cross-env MIGRATIONS=true ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm --config ./libs/api/database/src/ormconfig.ts",
    "typeorm-seed": "cross-env MIGRATIONS=true ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js --configName ./libs/api/database/src/ormconfig.ts",
    "migrate:create": "yarn typeorm migration:create -n",
    "migrate:generate": "yarn typeorm migration:generate -n",
    "migrate:run": "yarn typeorm migration:run",
    "migrate:revert": "yarn typeorm migration:revert",
    "migrate:prod": "NODE_ENV=production yarn typeorm migration:run",
    "seed:config": "yarn typeorm-seed config",
    "seed:run": "yarn typeorm-seed seed"
  },
  "private": true,
